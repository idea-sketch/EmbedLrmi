{
	"name": "EmbedLRMI",
	"version": "1.0",
	"author": [
		"Jan Böhme",
		"Uwe Schützenmeister"
	],
	"url": "https://idea-sketch.com",
	"description": "Embed LRMI metadata for articles retrieved from LRMI repository",
	"license-name": "GPL",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"MessagesDirs": {
		"EmbedLRMI": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"EmbedLRMIAlias": "EmbedLrmi.i18n.alias.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\EmbedLrmi\\": "src/"
	},
	"SpecialPages": {
		"PurgeLrmiCache": "MediaWiki\\Extension\\EmbedLrmi\\SpecialPurgeLrmiCache"
	},
	"AvailableRights": [
		"purgelrmicache"
	],
	"GroupPermissions": {
		"sysop": {
			"purgelrmicache": true
		},
		"bureaucrat": {
			"purgelrmicache": true
		}
	},
	"HookHandlers": {
		"EmbedLrmiHooks": {
			"class": "MediaWiki\\Extension\\EmbedLrmi\\EmbedLrmiHooks"
		},
		"EmbedLrmiData": {
			"class": "MediaWiki\\Extension\\EmbedLrmi\\EmbedLrmiData"
		}
	},
	"Hooks": {
		"BeforePageDisplay": "EmbedLrmiHooks",
		"SkinTemplateNavigation::Universal": "EmbedLrmiHooks",
		"SidebarBeforeOutput": "EmbedLrmiHooks",
		"PageSaveComplete": "EmbedLrmiData",
		"MediaWikiPerformAction": "EmbedLrmiData"
	},
	"manifest_version": 2
}